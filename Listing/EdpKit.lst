Nint32 WriteRemainlen(uint8 *buf, uint32 len_val, uint16 write_pos)
N{
N	
N	int32 remaining_count = 0;
N	uint8 byte = 0;
N
N	do
N	{
N		byte = len_val % 128;
N		len_val = len_val >> 7;
N		/* If there are more digits to encode, set the top bit of this digit */
N		if (len_val > 0)
N		{
N			byte = byte | 0x80;
N		}
N		buf[write_pos++] = byte;
N		remaining_count++;
N	} while(len_val > 0 && remaining_count < 5);
N
N	return --write_pos;
N}
N
Nint32 ReadRemainlen(int8 *buf, uint32 *len_val, uint16 read_pos)
N{
N	
N    uint32 multiplier = 1;
N    uint32 len_len = 0;
N    uint8 onebyte = 0;
N	
N    *len_val = 0;
N	
N    do
N	{
N        onebyte = buf[read_pos++];
N
N        *len_val += (onebyte & 0x7f) * multiplier;
N        multiplier <<= 7;
N
N        len_len++;
N        if (len_len > 4)
N		{
N            return -1;/*len of len more than 4;*/
N        }
N    } while((onebyte & 0x80) != 0);
N	
N    return read_pos;
N}
