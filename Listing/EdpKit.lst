N//==========================================================
N//	函数名称：	EDP_UnPacketPushData
N//
N//	函数功能：	PushData功能解包
N//
N//	入口参数：	rev_data：收到的数据
N//				src_devid：源devid缓存
N//				req：命令缓存
N//				req_len：命令长度
N//
N//	返回参数：	0-成功		1-失败
N//
N//	说明：		
N//==========================================================
Nuint8 EDP_UnPacketPushData(uint8 *rev_data, int8 **src_devid, int8 **req, uint32 *req_len)
N{
N
N	int32 read_pos = 0;
N	uint32 remain_len = 0;
N	uint16 devid_len = 0;
N	
N	//Byte0：PushData消息------------------------------------------------------------
N	if(rev_data[read_pos++] != PUSHDATA)
X	if(rev_data[read_pos++] != 0x30)
N		return 1;
N
N	//读取剩余长度--------------------------------------------------------------------
N	read_pos = ReadRemainlen((int8 *)rev_data, &remain_len, read_pos);
N	if(read_pos == -1)
N		return 2;
N	
N	//读取源devid长度-----------------------------------------------------------------
N	devid_len = (uint16)rev_data[read_pos] << 8 | rev_data[read_pos + 1];
N	read_pos += 2;
N
N	//分配内存------------------------------------------------------------------------
N	*src_devid = (int8 *)EDP_MallocBuffer(devid_len + 1);
X	*src_devid = (int8 *)malloc(devid_len + 1);
N	if(*src_devid == NULL)
X	if(*src_devid == 0)
N		return 3;
N
N	//读取源devid---------------------------------------------------------------------
N	memset(*src_devid, 0, devid_len + 1);
N	memcpy(*src_devid, (const int8 *)rev_data + read_pos, devid_len);
N	read_pos += devid_len;
N
N	remain_len -= 2 + devid_len;
N
N	//分配内存------------------------------------------------------------------------
N	*req = (int8 *)EDP_MallocBuffer(remain_len + 1);
X	*req = (int8 *)malloc(remain_len + 1);
N	if(*req == NULL)
X	if(*req == 0)
N	{
N		EDP_FreeBuffer(*src_devid);
X		free(*src_devid);
N		return 4;
N	}
N
N	//读取命令------------------------------------------------------------------------
N	memset(*req, 0, remain_len + 1);
N	memcpy(*req, (const int8 *)rev_data + read_pos, remain_len);
N	read_pos += remain_len;
N
N	*req_len = remain_len;
N	
N	return 0;
N
N}
N
N//==========================================================
N//	函数名称：	EDP_UnPacketCmd
N//
N//	函数功能：	下发命令解包
N//
N//	入口参数：	rev_data：收到的数据
N//				cmdid：cmdid
N//				cmdid_len：cmdid长度
N//				req：命令
N//				req_len：命令长度
N//
N//	返回参数：	0-成功		1-失败
N//
N//	说明：		
N//==========================================================
Nuint8 EDP_UnPacketCmd(uint8 *rev_data, int8 **cmdid, uint16 *cmdid_len, int8 **req, uint32 *req_len)
N{
N
N	uint32 remain_len = 0;
N	int32 read_pos = 0;
N	
N	//Byte0：PushData消息------------------------------------------------------------
N	if(rev_data[read_pos++] != CMDREQ)
X	if(rev_data[read_pos++] != 0xA0)
N		return 1;
N	
N	//读取剩余长度--------------------------------------------------------------------
N	read_pos = ReadRemainlen((int8 *)rev_data, &remain_len, read_pos);
N	if(read_pos == -1)
N		return 2;
N	
N	//读取cmdid长度-------------------------------------------------------------------
N	*cmdid_len = (uint16)rev_data[read_pos] << 8 | rev_data[read_pos + 1];
N	read_pos += 2;
N	
N	//分配内存------------------------------------------------------------------------
N	*cmdid = (int8 *)EDP_MallocBuffer(*cmdid_len + 1);
X	*cmdid = (int8 *)malloc(*cmdid_len + 1);
N	if(*cmdid == NULL)
X	if(*cmdid == 0)
N		return 3;
N	
N	//读取cmdid-----------------------------------------------------------------------
N	memset(*cmdid, 0, *cmdid_len + 1);
N	memcpy(*cmdid, (const int8 *)rev_data + read_pos, *cmdid_len);
N	read_pos += *cmdid_len;
N	
N	//读取req长度---------------------------------------------------------------------
N	*req_len = (uint32)rev_data[read_pos] << 24 | (uint32)rev_data[read_pos + 1] << 16
N					 | (uint32)rev_data[read_pos + 2] << 8 | (uint32)rev_data[read_pos + 3];
N	read_pos += 4;
N	
N	//分配内存------------------------------------------------------------------------
N	*req = (int8 *)EDP_MallocBuffer(*req_len + 1);
X	*req = (int8 *)malloc(*req_len + 1);
N	if(*req == NULL)
X	if(*req == 0)
N	{
N		EDP_FreeBuffer(*cmdid);
X		free(*cmdid);
N		return 4;
N	}
N	
N	//读取req-------------------------------------------------------------------------
N	memset(*req, 0, *req_len + 1);
N	memcpy(*req, (const int8 *)rev_data + read_pos, *req_len);
N	read_pos += *req_len;
N	
N	return 0;
N
N}
N
N//==========================================================
N//	函数名称：	EDP_PacketCmdResp
N//
N//	函数功能：	命令回复组包
N//
N//	入口参数：	cmdid：命令的cmdid(随命令下发)
N//				cmdid_len：cmdid长度
N//				req：命令
N//				req_len：命令长度
N//				edpPacket：包指针
N//
N//	返回参数：	0-成功		1-失败
N//
N//	说明：		
N//==========================================================
Nuint1 EDP_PacketCmdResp(const int8 *cmdid, uint16 cmdid_len, const int8 *resp, uint32 resp_len, EDP_PACKET_STRUCTURE *edpPacket)
N{
N	
N	uint32 remain_len = cmdid_len + resp_len + (resp_len ? 6 : 2);
N	
N	EDP_NewBuffer(edpPacket, remain_len + 5);
N	if(edpPacket->_data == NULL)
X	if(edpPacket->_data == 0)
N		return 1;
N	
N	//Byte0：CMDRESP消息------------------------------------------------------------
N	edpPacket->_data[edpPacket->_len++] = CMDRESP;
X	edpPacket->_data[edpPacket->_len++] = 0xB0;
N	
N	//写入剩余长度------------------------------------------------------------------
N	edpPacket->_len += WriteRemainlen(edpPacket->_data, remain_len, edpPacket->_len);
N	
N	//写入cmdid长度------------------------------------------------------------------
N	edpPacket->_data[edpPacket->_len++] = cmdid_len >> 8;
N	edpPacket->_data[edpPacket->_len++] = cmdid_len & 0x00FF;
N	
N	//写入cmdid----------------------------------------------------------------------
N	memcpy((int8 *)edpPacket->_data + edpPacket->_len, cmdid, cmdid_len);
N	edpPacket->_len += cmdid_len;
N	
N	if(resp_len)
N	{
N		//写入req长度-----------------------------------------------------------------
N		edpPacket->_data[edpPacket->_len++] = (uint8)(resp_len >> 24);
N		edpPacket->_data[edpPacket->_len++] = (uint8)(resp_len >> 16);
N		edpPacket->_data[edpPacket->_len++] = (uint8)(resp_len >> 8);
N		edpPacket->_data[edpPacket->_len++] = (uint8)(resp_len & 0x00FF);
N		
N		//写入req---------------------------------------------------------------------
N		memcpy((int8 *)edpPacket->_data + edpPacket->_len, resp, resp_len);
N		
N		edpPacket->_len += resp_len;
N	}
N
N	return 0;
N
N}
