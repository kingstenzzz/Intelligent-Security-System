N//==========================================================
N//	函数名称：	EDP_NewBuffer
N//
N//	函数功能：	申请内存
N//
N//	入口参数：	edpPacket：包结构体
N//				size：大小
N//
N//	返回参数：	无
N//
N//	说明：		1.可使用动态分配来分配内存
N//				2.可使用局部或全局数组来指定内存
N//==========================================================
Nvoid EDP_NewBuffer(EDP_PACKET_STRUCTURE *edpPacket, uint32 size)
N{
N	
N	uint32 i = 0;
N
N	if(edpPacket->_data == NULL)
X	if(edpPacket->_data == 0)
N	{
N		edpPacket->_memFlag = MEM_FLAG_ALLOC;
X		edpPacket->_memFlag = 1;
N		
N		edpPacket->_data = (uint8 *)EDP_MallocBuffer(size);
X		edpPacket->_data = (uint8 *)malloc(size);
N		if(edpPacket->_data != NULL)
X		if(edpPacket->_data != 0)
N		{
N			edpPacket->_len = 0;
N			
N			edpPacket->_size = size;
N
N			for(; i < edpPacket->_size; i++)
N				edpPacket->_data[i] = 0;
N		}
N	}
N	else
N	{
N		edpPacket->_memFlag = MEM_FLAG_STATIC;
X		edpPacket->_memFlag = 2;
N		
N		for(; i < edpPacket->_size; i++)
N			edpPacket->_data[i] = 0;
N		
N		edpPacket->_len = 0;
N		
N		if(edpPacket->_size < size)
N			edpPacket->_data = NULL;
X			edpPacket->_data = 0;
N	}
N
N}
