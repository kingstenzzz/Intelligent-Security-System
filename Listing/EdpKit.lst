N//==========================================================
N//	函数名称：	EDP_PacketSaveJson
N//
N//	函数功能：	封装协议头
N//
N//	入口参数：	devid：设备ID(可为空)
N//				send_buf：json缓存buf
N//				send_len：json总长
N//				type_bin_head：bin文件的消息头
N//				type：类型
N//				edpPacket：包指针
N//
N//	返回参数：	0-成功		1-失败
N//
N//	说明：		当不为Type2的时候，type_bin_head可为NULL
N//==========================================================
Nuint8 EDP_PacketSaveData(const int8 *devid, int32 send_len, int8 *type_bin_head, SaveDataType type, EDP_PACKET_STRUCTURE *edpPacket)
N{
N
N	int32 remain_len = 0;
N	uint8 devid_len = strlen(devid);
N	
N	if(type == 2 && type_bin_head == NULL)
X	if(type == 2 && type_bin_head == 0)
N		return 1;
N	
N	if(type == 2)
N		EDP_NewBuffer(edpPacket, strlen(type_bin_head));
N	else
N		EDP_NewBuffer(edpPacket, send_len + 24);
N	if(edpPacket->_data == NULL)
X	if(edpPacket->_data == 0)
N		return 2;
N
N	//Byte0：消息类型--------------------------------------------------------------
N	edpPacket->_data[edpPacket->_len++] = SAVEDATA;
X	edpPacket->_data[edpPacket->_len++] = 0x80;
N	
N	if(devid)
N	{
N		if(type == 2)
N			remain_len = 12 + strlen(type_bin_head) + send_len;
N		else
N			remain_len = 8 + send_len + devid_len;
N		
N		//剩余消息长度-------------------------------------------------------------
N		edpPacket->_len += WriteRemainlen(edpPacket->_data, remain_len, edpPacket->_len);
N		
N		//标志--bit7:1-有devid，0-无devid		bit6:1-有消息编号，0-无消息编号----
N		edpPacket->_data[edpPacket->_len++] = 0xC0;
N		
N		//DEVID长度---------------------------------------------------------------
N		edpPacket->_data[edpPacket->_len++] = 0;
N		edpPacket->_data[edpPacket->_len++] = devid_len;
N		
N		//DEVID------------------------------------------------------------------
N		strncat((int8 *)edpPacket->_data + edpPacket->_len, devid, devid_len);
N		edpPacket->_len += devid_len;
N		
N		//消息编号----------------------------------------------------------------
N		edpPacket->_data[edpPacket->_len++] = MSG_ID_HIGH;
X		edpPacket->_data[edpPacket->_len++] = 0x55;
N		edpPacket->_data[edpPacket->_len++] = MSG_ID_LOW;
X		edpPacket->_data[edpPacket->_len++] = 0xAA;
N	}
N	else
N	{
N		if(type == 2)
N			remain_len = 10 + strlen(type_bin_head) + send_len;
N		else
N			remain_len = 6 + send_len;
N		 
N		//剩余消息长度------------------------------------------------------------
N		edpPacket->_len += WriteRemainlen(edpPacket->_data, remain_len, edpPacket->_len);
N		
N		//标志--bit7:1-有devid，0-无devid		bit6:1-有消息编号，0-无消息编号----
N		edpPacket->_data[edpPacket->_len++] = 0x40;
N		
N		//消息编号----------------------------------------------------------------
N		edpPacket->_data[edpPacket->_len++] = MSG_ID_HIGH;
X		edpPacket->_data[edpPacket->_len++] = 0x55;
N		edpPacket->_data[edpPacket->_len++] = MSG_ID_LOW;
X		edpPacket->_data[edpPacket->_len++] = 0xAA;
N	}
N	
N	edpPacket->_data[edpPacket->_len++] = type;
N	
N	if(type == 2)
N	{
N		uint8 type_bin_head_len = strlen(type_bin_head);
N		uint8 i = 0;
N		
N		//消息头长度---------------------------------------------------------------
N		edpPacket->_data[edpPacket->_len++] = MOSQ_MSB(type_bin_head_len);
X		edpPacket->_data[edpPacket->_len++] = (uint8)((type_bin_head_len & 0xFF00) >> 8);
N		edpPacket->_data[edpPacket->_len++] = MOSQ_LSB(type_bin_head_len);
X		edpPacket->_data[edpPacket->_len++] = (uint8)(type_bin_head_len & 0x00FF);
N		
N		//消息头-------------------------------------------------------------------
N		for(; i < type_bin_head_len; i++)
N			edpPacket->_data[edpPacket->_len++] = type_bin_head[i];
N		
N		//图片长度-----------------------------------------------------------------
N		edpPacket->_data[edpPacket->_len++] = (uint8)(send_len >> 24);
N		edpPacket->_data[edpPacket->_len++] = (uint8)(send_len >> 16);
N		edpPacket->_data[edpPacket->_len++] = (uint8)(send_len >> 8);
N		edpPacket->_data[edpPacket->_len++] = (uint8)send_len;
N	}
N	else
N	{
N		//json长度-----------------------------------------------------------------
N		edpPacket->_data[edpPacket->_len++] = MOSQ_MSB(send_len);
X		edpPacket->_data[edpPacket->_len++] = (uint8)((send_len & 0xFF00) >> 8);
N		edpPacket->_data[edpPacket->_len++] = MOSQ_LSB(send_len);
X		edpPacket->_data[edpPacket->_len++] = (uint8)(send_len & 0x00FF);
N	}
N	
N	return 0;
N
N}
N//==========================================================
N//	函数名称：	EDP_PacketPushData
N//
N//	函数功能：	PushData功能组包
N//
N//	入口参数：	devid：设备ID
N//				msg：推送数据
N//				msg_len：推送的数据长度
N//				edpPacket：包指针
N//
N//	返回参数：	0-成功		1-失败
N//
N//	说明：		
N//==========================================================
Nuint8 EDP_PacketPushData(const int8 *devid, const int8 *msg, uint32 msg_len, EDP_PACKET_STRUCTURE *edpPacket)
N{
N	
N	uint32 remain_len = 2 + strlen(devid) + msg_len;
N	uint8 devid_len = strlen(devid);
N	uint16 i = 0;
N	uint16 size = 5 + strlen(devid) + msg_len;
N	
N	if(devid == NULL || msg == NULL || msg_len == 0)
X	if(devid == 0 || msg == 0 || msg_len == 0)
N		return 1;
N
N	EDP_NewBuffer(edpPacket, size);
N	if(edpPacket->_data == NULL)
X	if(edpPacket->_data == 0)
N		return 2;
N	
N	//Byte0：pushdata类型-----------------------------------------------------------
N	edpPacket->_data[edpPacket->_len++] = PUSHDATA;
X	edpPacket->_data[edpPacket->_len++] = 0x30;
N	
N	//剩余长度----------------------------------------------------------------------
N	edpPacket->_len += WriteRemainlen(edpPacket->_data, remain_len, edpPacket->_len);
N	
N	//DEVID长度---------------------------------------------------------------------
N	edpPacket->_data[edpPacket->_len++] = MOSQ_MSB(devid_len);
X	edpPacket->_data[edpPacket->_len++] = (uint8)((devid_len & 0xFF00) >> 8);
N	edpPacket->_data[edpPacket->_len++] = MOSQ_LSB(devid_len);
X	edpPacket->_data[edpPacket->_len++] = (uint8)(devid_len & 0x00FF);
N	
N	//写入DEVID---------------------------------------------------------------------
N	for(; i < devid_len; i++)
N		edpPacket->_data[edpPacket->_len++] = devid[i];
N	
N	//写入数据----------------------------------------------------------------------
N	for(i = 0; i < msg_len; i++)
N		edpPacket->_data[edpPacket->_len++] = msg[i];
N	
N	return 0;
N
N}
