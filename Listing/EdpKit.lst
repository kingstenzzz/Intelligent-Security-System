N//	返回参数：	0-成功		1-失败
N//
N//	说明：		
N//==========================================================
Nuint1 EDP_PacketConnect1(const int8 *devid, const int8 *apikey, uint16 cTime, EDP_PACKET_STRUCTURE *edpPacket)
N{
N	
N	uint8 devid_len = strlen(devid);
N	uint8 apikey_len = strlen(apikey);
N
N	//分配内存---------------------------------------------------------------------
N	EDP_NewBuffer(edpPacket, 56);
N	if(edpPacket->_data == NULL)
X	if(edpPacket->_data == 0)
N		return 1;
N	
N	//Byte0：连接类型--------------------------------------------------------------
N	edpPacket->_data[0] = CONNREQ;
X	edpPacket->_data[0] = 0x10;
N	edpPacket->_len++;
N	
N	//Byte1：剩余消息长度----------------------------------------------------------
N	edpPacket->_data[1] = 13 + devid_len + apikey_len;
N	edpPacket->_len++;
N	
N	//Byte2~3：协议名长度----------------------------------------------------------
N	edpPacket->_data[2] = 0;
N	edpPacket->_data[3] = 3;
N	edpPacket->_len += 2;
N	
N	//Byte4~6：协议名--------------------------------------------------------------
N	strncat((int8 *)edpPacket->_data + 4, "EDP", 3);
N	edpPacket->_len += 3;
N	
N	//Byte7：协议版本--------------------------------------------------------------
N	edpPacket->_data[7] = 1;
N	edpPacket->_len++;
N	
N	//Byte8：连接标志--------------------------------------------------------------
N	edpPacket->_data[8] = 0x40;
N	edpPacket->_len++;
N	
N	//Byte9~10：连接保持时间-------------------------------------------------------
N	edpPacket->_data[9] = MOSQ_MSB(cTime);
X	edpPacket->_data[9] = (uint8)((cTime & 0xFF00) >> 8);
N	edpPacket->_data[10] = MOSQ_LSB(cTime);
X	edpPacket->_data[10] = (uint8)(cTime & 0x00FF);
N	edpPacket->_len += 2;
N	
N	//Byte11~12：DEVID长度---------------------------------------------------------
N	edpPacket->_data[11] = MOSQ_MSB(devid_len);
X	edpPacket->_data[11] = (uint8)((devid_len & 0xFF00) >> 8);
N	edpPacket->_data[12] = MOSQ_LSB(devid_len);
X	edpPacket->_data[12] = (uint8)(devid_len & 0x00FF);
N	edpPacket->_len += 2;
N	
N	//Byte13~13+devid_len：DEVID---------------------------------------------------
N	strncat((int8 *)edpPacket->_data + 13, devid, devid_len);
N	edpPacket->_len += devid_len;
N	
N	//Byte13+devid_len~13+devid_len+2：APIKEY长度----------------------------------
N	edpPacket->_data[13 + devid_len] = MOSQ_MSB(apikey_len);
X	edpPacket->_data[13 + devid_len] = (uint8)((apikey_len & 0xFF00) >> 8);
N	edpPacket->_data[14 + devid_len] = MOSQ_LSB(apikey_len);
X	edpPacket->_data[14 + devid_len] = (uint8)(apikey_len & 0x00FF);
N	edpPacket->_len += 2;
N	
N	//Byte15+devid_len~15+devid_len+apikey_len：APIKEY-----------------------------
N	strncat((int8 *)edpPacket->_data + 15 + devid_len, apikey, apikey_len);
N	edpPacket->_len += apikey_len;
N	
N	return 0;
N
N}
