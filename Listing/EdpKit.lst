X	edpPacket->_data[9] = (uint8)((cTime & 0xFF00) >> 8);
N	edpPacket->_data[10] = MOSQ_LSB(cTime);
X	edpPacket->_data[10] = (uint8)(cTime & 0x00FF);
N	edpPacket->_len += 2;
N	
N	//Byte11~12：DEVID长度---------------------------------------------------------
N	edpPacket->_data[11] = 0;
N	edpPacket->_data[12] = 0;
N	edpPacket->_len += 2;
N	
N	//Byte13~14：PROID长度---------------------------------------------------------
N	edpPacket->_data[13] = MOSQ_MSB(proid_len);
X	edpPacket->_data[13] = (uint8)((proid_len & 0xFF00) >> 8);
N	edpPacket->_data[14] = MOSQ_LSB(proid_len);
X	edpPacket->_data[14] = (uint8)(proid_len & 0x00FF);
N	edpPacket->_len += 2;
N	
N	//Byte15~15+proid_len：RPOID---------------------------------------------------
N	strncat((int8 *)edpPacket->_data + 15, proid, proid_len);
N	edpPacket->_len += proid_len;
N	
N	//Byte15+devid_len~15+proid_len+1：APIKEY长度----------------------------------
N	edpPacket->_data[15 + proid_len] = MOSQ_MSB(authkey_len);
X	edpPacket->_data[15 + proid_len] = (uint8)((authkey_len & 0xFF00) >> 8);
N	edpPacket->_data[16 + proid_len] = MOSQ_LSB(authkey_len);
X	edpPacket->_data[16 + proid_len] = (uint8)(authkey_len & 0x00FF);
N	edpPacket->_len += 2;
N	
N	//Byte17+proid_len~17+proid_len+apikey_len：APIKEY-----------------------------
N	strncat((int8 *)edpPacket->_data + 17 + proid_len, auth_key, authkey_len);
N	edpPacket->_len += authkey_len;
N	
N	return 0;
N
N}
